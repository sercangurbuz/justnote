{
  "name": "justnote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 8080",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev --port 8080",
    "build": "next build",
    "build:i18n": "npm run extract:i18n && npm run compile:i18n && next build",
    "start": "next start --port 8080",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "extract:i18n": "formatjs extract \"{pages,src}/*.{js,ts,tsx}\" --format simple --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file lang/tr.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang",
    "clean": "rimraf .next dist",
    "lint": "next lint",
    "gql:generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@auth0/nextjs-auth0": "^1.5.0",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@formatjs/cli": "^4.3.1",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "next",
    "@mui/x-data-grid": "^4.0.0",
    "formik": "^2.2.9",
    "graphql": "^15.6.0",
    "http-proxy-middleware": "^2.0.1",
    "next": "11.1.0",
    "next-connect": "^0.10.2",
    "next-with-apollo": "^5.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/near-operation-file-preset": "^2.1.4",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@types/react": "17.0.19",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "typescript": "4.4.2"
  }
}
